{"main":"function Bell(x, y) {\n    let img = app.getResource('bell');\n    let sound = app.getResource('ding.mp3');\n\n    this.width = img.width;\n    this.height = img.height;\n    this.x = x-this.width/2;\n    this.y = y-this.height/2;\n\n    this.draw = function(ctx) {\n        ctx.drawImage(img, this.x, this.y, this.width, this.height);\n    }\n    this.play = function() { sound.play(); }\n    this.playing = function() { return !sound.paused; }\n    this.currentTime = function() { return sound.currentTime; }\n    this.boundsContain = function(pos) {\n        if(pos.x<this.x || pos.x >=this.x+this.width)\n            return false;\n        return pos.y>=this.y && pos.y <this.y+this.height;\n    }\n};\n\nlet ctx = app.getCanvasContext();\nlet bell = new Bell(app.width/2, app.height/2);\n\napp.addEventListener('update', (evt)=>{\n    ctx.clearRect(0,0, app.width, app.height);\n    if(bell.playing()) {\n        const r = bell.width * Math.pow(1+bell.currentTime(), 5) - bell.width;\n        const cx = bell.x+bell.width/2, cy=bell.y+bell.height/2;\n        ctx.save();\n        ctx.strokeStyle = 'yellow';\n        ctx.lineWidth = 2;\n        ctx.beginPath();\n        ctx.arc(cx,cy, r, 0, 2*Math.PI, true);\n        ctx.closePath();\n        ctx.stroke();\n        ctx.restore();\n    }\n    bell.draw(ctx);\n});\napp.addEventListener('resize', (width, height, orientation)=>{\n   bell.x = width/2-bell.width/2;\n   bell.y = height/2-bell.height/2;\n});\napp.addEventListener('pointer', (type, x, y)=>{\n    if(bell.boundsContain({x:x, y:y}) && type=='start' && !bell.playing())\n        bell.play();\n});\n","resources":{"ding.mp3":"data:audio/mp3;base64,","bell":"data:image/png;base64,"}}